    "Filament Ternary Filter": {
        "prefix": [
            "ternaryfilter",
            "filterternary",
            "tf",
            "ft"
        ],
        "body": [
            "TernaryFilter::make('$1')$0,"
        ],
        "description": "Ternary filter for table builder."
    },
    "Filament Filter with Custom Form": {
        "prefix": [
            "formfilter",
            "filterform",
            "ff",
            "ff"
        ],
        "body": [
            "Filter::make('$1')",
            "\t->form(${2:[\n\t\t$3\n\t]})",
            "\t->query(function (Builder \\$query, array \\$data): Builder {",
            "\t\t\\$query${4:->}",
            "\t})$0"
        ],
        "description": "Filter with custom form for table builder."
