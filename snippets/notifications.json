{
    "Filament Notification": {
        "prefix": [
            "notification"
        ],
        "body": [
            "Notification::make()",
            "\t->title('$1')${2:",
            "\t->body('$3')}${4:",
            "\t->success()}${5:",
            "\t->icon('$6')}${7:",
            "\t->iconColor('$8')}${9:",
            "\t->duration(${10:5000})}${11:",
            "\t->persistent()}${12:",
            "\t->actions([\n\t\t$13\n\t])}",
            "\t->send();"
        ],
        "description": "Create a filament notification."
    },
