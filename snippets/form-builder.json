{
    //Form Components
    //Text Inputs
    "Filament Text Input": {
        "prefix": [
            "textinput",
            "ti",
            "it"
        ],
        "body": [
            "TextInput::make('$1')$0,"
        ],
        "description": "Text input for form builder."
    },
    "Filament Text Input with Mask": {
        "prefix": [
            "textinputmask",
            "tim",
            "itm"
        ],
        "body": [
            "TextInput::make('$1')",
            "\t->mask(fn (TextInput\\Mask \\$mask) => \\$mask->pattern('${2:(000)000-00-00}'))$0,"
        ],
        "description": "Text input with mask for form builder."
    },
    "Filament Email Text Input": {
        "prefix": [
